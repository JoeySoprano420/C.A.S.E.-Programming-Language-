{
  "name": "C.A.S.E. Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "editorLineNumber.foreground": "#858585",
    "editorCursor.foreground": "#aeafad"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#6A9955",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.control", "keyword.other"],
      "settings": {
        "foreground": "#C586C0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.function.math", "support.function.string", "support.function.collection"],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#569CD6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "scope": ["keyword.control.terminator"],
      "settings": {
        "foreground": "#FF6B6B",
        "fontStyle": "bold"
      }
    }
  ]
}
